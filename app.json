{
  "name": "klist",
  "description": "A simple deployment of Alist",
  "repository": "https://github.com/krishd9895/KList.git",
  "stack": "container",
  "env": {
    "DB_TYPE": {
      "description": "Type of the database",
      "value": "sqlite3"
    },
    "DB_HOST": {
      "description": "Database host address",
      "value": "",
      "required": false
    },
    "DB_USER": {
      "description": "Database username",
      "value": "",
      "required": false
    },
    "DB_PASS": {
      "description": "Database password",
      "value": "",
      "required": false
    },
    "DB_TABLE_PREFIX": {
      "description": "Table prefix for the database",
      "value": "alist_"
    },
    "CDN": {
      "description": "Content Delivery Network",
      "value": "https://cdn.jsdelivr.net/npm/alist-web@$version/dist"
    },
    "DB_PORT": {
      "description": "Database port",
      "value": "3306"
    },
    "PORT": {
      "description": "Port the app will run on",
      "value": "5244"
    },
    "DB_NAME": {
      "description": "Database name",
      "value": "",
      "required": false
    },
    "DB_SSL_MODE": {
      "description": "Database SSL mode",
      "value": "",
      "required": false
    }
  }
}
